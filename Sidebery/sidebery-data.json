  "sidebarCSS": "#root.root {--toolbar-el-overlay-hover-bg: rgba(255,255,255,.2);}\n#root.root {--toolbar-el-overlay-active-bg: rgba(255,255,255,0.2);}\n#root.root {--tabs-activated-bg: rgb(255,255,255,0.1);}\n#root.root {--tabs-activated-shadow: ;}\n#root.root {--tabs-pinned-height: 40px;}\n#root.root {--tabs-pinned-width: 40px;}\n#root.root {--tabs-indent: 20px;}\n#root.root {--tabs-inner-gap: 6px;}\n\n\n#root.root {--toolbar-el-overlay-clicked-bg: rgba(255,255,255,0.2);}\n\n#root.root {--nav-btn-active-shadow: ;}\n#root.root {--general-border-radius: 8px;}\n#root.root {--general-margin: 10px;}\n\n/* ^^^^^^^^^^^^^^^^^^^^^^^^ */\n/* --- Sidebery Toggles --- */\n\n/* --- My Custom CSS --- */\n/* vvvvvvvvvvvvvvvvvvvvv */\n\n/* Use pointer cursor */\n.Tab {\n\tcursor: pointer;\n}\n\n/* Unpinned tabs */\n.Tab[data-pin=\"false\"] {\n\n\t/* Container indicator */\n\t.ctx {\n\t\twidth: 4px;\n\t\theight: 16px;\n\t\ttop: 50%;\n\t\ttransform: translateY(-50%);\n\t\tright: -8px;\n\t\tborder-radius: 999px;\n\t}\n}\n\n/* Pinned tabs */\n.Tab[data-pin=\"true\"] {\n\t--tabs-border-radius: 8px;\n\t--tabs-normal-bg: rgba(255,255,255,0.07);\n\n\t/* Container indicator */\n\t.ctx {\n\t\tbottom: 4px !important;\n\t\tleft: 8px !important;\n\t\tright: 8px !important;\n\t\twidth: auto !important;\n\t\theight: 3px !important;\n\t\tborder-radius: 8px;\n\t}\n}",
